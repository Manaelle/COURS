
###################################################
## This Makefile has been generated by gen_make  ##
###################################################


CC=gcc
CFLAGS=-Wall -Werror
LDFLAGS=

LIBS=

DBG_PREF=dbg_

ifeq ($(MKFLDEBUG),1)
CFLAGS+=-g
PREFIX=$(DBG_PREF)
endif

EXEC=$(PREFIX)essai_fap 
ALL_EXEC=essai_fap $(DBG_PREF)essai_fap 

###################################################
################## Generic rules ##################

all: $(EXEC)

debug:
	@export MKFLDEBUG=1 ; \
	make all

$(EXEC):% : %.o
	$(CC) $(CFLAGS) -o $@ $(LDFLAGS) $^ $(LIBS)

$(PREFIX)%.o: %.c
	$(CC) $(CFLAGS) -o $@ -c $<

$(patsubst %,$(DBG_PREF)%,$(EXEC)):% :
	@export MKFLDEBUG=1 ; \
	make $@

###################################################
################ Set dependencies #################

$(PREFIX)essai_fap.o: fap.h 
$(PREFIX)essai_fap: $(PREFIX)fap.o 
$(PREFIX)fap.o: fap.h 

###################################################
###################### Clean ######################

clean:
	rm -f $(ALL_EXEC) *.o



###################################################
################## Other Formats ##################

%.tar: %
	tar -cvf $@ $<

%.tar.gz: %
	tar -cvzf $@ $<

%.pdf: %.tex
	pdflatex $<
